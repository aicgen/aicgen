{
  "name": "@aicgen/aicgen",
  "version": "1.0.2",
  "description": "AI Config Generator - Automated AI assistant configuration for your projects",
  "keywords": [
    "ai",
    "cli",
    "code-generator",
    "claude",
    "gemini",
    "openai",
    "config",
    "assistant"
  ],
  "author": "Lahiru Sandaruwan <lpsandaruwan@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aicgen/aicgen.git"
  },
  "bin": {
    "aicgen": "dist/index.js"
  },
  "type": "module",
  "engines": {
    "bun": ">=1.0.0"
  },
  "scripts": {
    "generate-version": "bun run scripts/generate-version.ts",
    "embed": "bun run generate-version && bun run scripts/embed-data.ts",
    "prebuild": "bun run embed",
    "build": "bun build src/index.ts --outdir dist --target node",
    "build:binary": "bun run embed && bun run scripts/build-binary.ts current",
    "build:binary:windows": "bun run embed && bun run scripts/build-binary.ts windows",
    "build:binary:linux": "bun run embed && bun run scripts/build-binary.ts linux",
    "build:binary:macos": "bun run embed && bun run scripts/build-binary.ts macos",
    "build:all": "bun run build:binary:windows && bun run build:binary:linux && bun run build:binary:macos",
    "dev": "bun --watch src/index.ts",
    "start": "bun src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@google/generative-ai": "^0.24.1",
    "@inquirer/checkbox": "^5.0.2",
    "@inquirer/prompts": "^6.0.1",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "decompress": "^4.2.1",
    "decompress-targz": "^4.1.1",
    "fast-glob": "^3.3.3",
    "openai": "^6.15.0",
    "ora": "^8.1.1",
    "winston": "^3.19.0",
    "yaml": "^2.6.1"
  },
  "devDependencies": {
    "@types/bun": "^1.1.13",
    "@types/decompress": "^4.2.7",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.17.9",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "jest": "^30.2.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.4.6",
    "typescript": "^5.7.2"
  }
}